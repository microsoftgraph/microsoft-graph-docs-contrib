{
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "breadcrumb/**",
          "concepts/sdks/snippets/dotnet/**",
          "concepts/sdks/snippets/go/**",
          "concepts/sdks/snippets/java/**",
          "concepts/sdks/snippets/python/**",
          "concepts/sdks/snippets/typescript/**",
          "concepts/includes/connecting-external-content/snippets/**",
          "concepts/tutorials/includes/dotnet/src/**",
          "concepts/tutorials/includes/go/src/**",
          "concepts/tutorials/includes/java/src/**",
          "concepts/tutorials/includes/javascript/src/**",
          "concepts/tutorials/includes/php/src/**",
          "concepts/tutorials/includes/power-automate/src/**",
          "concepts/tutorials/includes/powershell/src/**",
          "concepts/tutorials/includes/python/src/**",
          "concepts/tutorials/includes/typescript/src/**",
          "concepts/changelog-archive.md",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "src": "breadcrumb",
        "dest": "concepts/breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "rules": {},
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "Standard",
      "feedback_product_url": "https://developer.microsoft.com/graph/support",
      "feedback_github_repo": "microsoftgraph/microsoft-graph-docs-contrib",
      "breadcrumb_path": "/graph/concepts/breadcrumb/toc.json",
      "author": "MSGraphDocsVteam",
      "ms.author": "MSGraphDocsVteam",
      "uhfHeaderId": "MSDocsHeader-MSGraph",
      "ms.suite": "microsoft-graph",
      "ms.subservice": "non-product-specific",
      "searchScope": ["graph"],
      "toc_preview": true,
      "recommendations": false
    },
    "fileMetadata": {
      "ms.service": {
        "**/*.md": "microsoft-graph",
        "**/*.yml": "microsoft-graph"
      },
      "ms.topic": {
        "**/*.md": "conceptual"
      },
      "titleSuffix": {
        "**/*.md": "Microsoft Graph"
      }
    },
    "template": [],
    "dest": "microsoft-graph-docs",
    "markdownEngineName": "markdig"
  }
}
